---
import { cleanClass } from "../../utils/cleanClass";
const { label, href, variant = 'btn-primary', newTab = false } = Astro.props;

---

<a
  href={href}
  class={`btn ${cleanClass(variant)}`}
  target={newTab ? '_blank' : '_self'}
  rel={newTab ? 'noopener noreferrer' : undefined}
>
  {label}
</a>
